<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundProgressBar">  
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />  
        <attr name="textSize" format="dimension" /> 
        <attr name="max" format="integer"></attr> 
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomprogressBarView">
        <attr name="strokeWidth" format="dimension"/>
        <attr name="backColor" format="color"/>
        <attr name="textSizeProgress" format="dimension" />
        <attr name="progressColor" format="color"/>
        <attr name="strokeColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="page_titleBar">
        <attr name="leftImagResource" format="reference"></attr>
        <attr name="showLeftImag" format="boolean"></attr>
        <attr name="bg_color" format="color"></attr>
        <attr name="rightImagResource" format="reference"></attr>
        <attr name="showRightImag" format="boolean"></attr>
        <attr name="text" format="string|reference"></attr>
    </declare-styleable>
</resources>