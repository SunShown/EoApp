<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="4dp"/>
            <solid android:color="@color/nonet_press"/>
            <stroke android:color="@color/nonet_border"/>
        </shape>
    </item>
    <item android:state_pressed="false" >
        <shape>
            <corners android:radius="4dp"/>
            <solid android:color="@color/nonet_normal"/>
            <stroke
                android:width="1dp"
                android:color="@color/nonet_border"/>
        </shape>
    </item>
</selector>